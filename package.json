{
  "name": "eslint-plugin-node-cjs-extension",
  "version": "0.0.1",
  "description": "Check consistency in require() usage - load CJS modules with or without extensions.",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "Mark Ablovatskii <mark.ablov@gmail.com>",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint ."
